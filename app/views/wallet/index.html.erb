<table class="table">
  <thead>
  <tr>
    <th scope='col'>Score</th>
    <th scope='col'>Balance</th>
    <th scope='col'>Wallet Type</th>
    <th scope='col'>Transaction list</th>
    <th scope='col'>Withdraw</th>
  </tr>
  </thead>
  <tbody>
  <% @wallets.each do |record| %>
    <tr>
      <th scope="row"> <%= record.number %> </th>
      <th><%= record.balance %></th>
      <th><%= record.wallet_type_present %></th>
      <th><%= link_to 'Show', wallet_path(record.id), class: 'btn btn-primary' %></th>
      <th> <%= link_to 'Withdraw', transaction_new_withdraw_path(score: record.number), class: 'btn btn-warning' %> </th>
    </tr>
  <% end %>
  </tbody>
</table>


